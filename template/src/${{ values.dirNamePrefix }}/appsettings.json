{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "StatsD": {
    "HostName": "localhost",
    "Port": 8125,
    "Prefix": "${{ values.applicationName }}"
  },
  "ConnectionStrings": {
    "SqlServer": "Data Source=(LocalDB)\\MSSQLLocalDB;Initial Catalog=${{ values.applicationName }};Integrated Security=SSPI"
  },
  "CosmosDb": {
    "Endpoint": "https://feature-api-template-test.documents.azure.com:443/",
    "PrimaryKey": "<<ACCOUNT KEY>>",
    "DatabaseId": "${{ values.applicationName }}",
    "CreateDatabaseAndContainersIfNotExists": "true"
  }
}
