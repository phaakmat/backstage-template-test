{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
  {%- if values.enableCosmosDb %},
  "CosmosDb": {
    "Endpoint": "<<ENDPOINT>>",
    "PrimaryKey": "<<PRIMARY KEY>>",
    "DatabaseId": "<<DATABASE ID>>",
    "CreateDatabaseAndContainersIfNotExists": "true"
  }
  {%- endif %}
}
